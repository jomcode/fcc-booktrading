    "clean-webpack-plugin": "^0.1.9",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
